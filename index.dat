            { name: "–í–µ—Ä—Ö–Ω–∏–π –∏–Ω–¥–µ–∫—Å", symbols: ["‚Å∞","¬π","¬≤","¬≥","‚Å¥","‚Åµ","‚Å∂","‚Å∑","‚Å∏","‚Åπ","‚Å∫","‚Åª","Àü","/","‚Åº","‚ÅΩ","‚Åæ"], example: "¬≤‚É∞‚ÅΩ¬≥‚Åª¬π‚Åæ=‚Å¥" },
            { name: "–ù–∏–∂–Ω–∏–π –∏–Ω–¥–µ–∫—Å", symbols: ["‚ÇÄ","‚ÇÅ","‚ÇÇ","‚ÇÉ","‚ÇÑ","‚ÇÖ","‚ÇÜ","‚Çá","‚Çà","‚Çâ","‚Çä","‚Çã","‚Çì","/","‚Çå","‚Çç","‚Çé"], example: "‚ÇÇ‚Çì‚Çç‚ÇÉ‚Çã‚ÇÅ‚Çé=‚ÇÑ" },
            { name: "–° —Ç–æ—á–∫–æ–π", symbols: ["üÑÄ","‚íà","‚íâ","‚íä","‚íã","‚íå","‚íç","‚íé","‚íè","‚íê","+","-","*","/","=","(",")"], example: "‚íâ*(‚íä-‚íà)=‚íã" },
            { name: "üé≤ –°–ª—É—á–∞–π–Ω—ã–π —Å—Ç–∏–ª—å", symbols: ["üé≤","üé≤","üé≤","üé≤","üé≤","üé≤","üé≤","üé≤","üé≤","üé≤","üé≤","üé≤","üé≤","üé≤","üé≤","üé≤","üé≤"],
    example: "üé≤√ó(üé≤-üé≤)=üé≤ (—Å—Ç–∏–ª–∏ –º–µ–Ω—è—é—Ç—Å—è!)"
}
        ];

        // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è —Å–µ–ª–µ–∫—Ç–æ—Ä–∞ —Å—Ç–∏–ª–µ–π
        function initializeStyles() {
            const select = document.getElementById('styleSelect');
            styles.forEach((style, index) => {
                const option = document.createElement('option');
                option.value = index;
                option.textContent = `${index}: ${style.example}`;
                select.appendChild(option);
            });
            select.value = 2;
        }

        // –ù–ï–í–ò–î–ò–ú–ê–Ø –ó–ê–©–ò–¢–ê - –¥–µ—Ç–µ–∫—Ç–æ—Ä –∫–≤–∞–¥—Ä–∞—Ç–Ω—ã—Ö —Å–∏–º–≤–æ–ª–æ–≤
        function isSquareSymbolAt(text, position) {
            if (position > text.length - 3) return false;
            
            const char = text[position];
            const modifier = text[position + 1];
            const variation = text[position + 2];
            
            return (char >= '0' && char <= '9') && 
                   modifier === 'Ô∏è' && 
                   variation === '‚É£';
        }

        // –í–ï–õ–ò–ö–û–õ–ï–ü–ù–û–ï –ü–†–ï–û–ë–†–ê–ó–û–í–ê–ù–ò–ï —Å –ø—Ä–∞–≤–∏–ª–æ–º –Æ—Ä–∏—è –Æ—Ä—å–µ–≤–∏—á–∞ (–Ω–∏—á–µ–≥–æ –Ω–µ –∑–∞–ø—Ä–µ—â–µ–Ω–æ, –≤—ã—Ö–æ–¥—è—â–µ–µ –∑–∞ —Ä–∞–º–∫–∏ –∏–≥–Ω–æ—Ä–∏—Ä—É–µ—Ç—Å—è)
        function ascii2Unicode(text, styleIndex = 2) {
            if (!text) return "";
            
            if (styleIndex === 0) {
                return unicodeToAscii(text);
            }
            
            const asciiSymbols = styles[0].symbols;
            
            let result = "";
            let i = 0;
            
            while (i < text.length) {
                let char = text[i];
                let found = false;
                
                
                // –ï—Å–ª–∏ –ø–æ—Å–ª–µ–¥–Ω–∏–π —Å—Ç–∏–ª—å –°–ª—É—á–∞–π–Ω—ã–π, –≤—ã–±–∏—Ä–∞–µ–º –∫–∞–∫–æ–π-–Ω–∏–±—É–¥—å –∏–∑ –¥–µ—Ç–µ—Ä–º–∏–Ω—Ç—Ä–æ–≤–∞–Ω–Ω—ã—Ö, –∫—Ä–æ–º–µ 0
                realStyleIndex = styleIndex === (styles.length - 1) ? Math.floor(Math.random() * (styles.length - 2)) + 1 : styleIndex;

                if (isSquareSymbolAt(text, i)) {
                    result += text.substring(i, i + 3); // –ò–≥–Ω–æ—Ä–∏—Ä—É–µ–º —ç–º–æ–¥–∑–∏ (–∫–≤–∞–¥—Ä–∞—Ç–Ω—ã–µ —Å–∏–º–≤–æ–ª—ã —Å —Ü–∏—Ñ—Ä–∞–º–∏)
                    i += 3;
                    continue;
                }
                
                for (let j = 0; j < asciiSymbols.length; j++) {
                    if (char === asciiSymbols[j]) {
                        char = styles[realStyleIndex].symbols[j];
                        found = true;
                        break;
                    }
                }
                
                result += char;
                i++;
            }
            
            return result;
        }

        // –°–£–ü–ï–†-–ü–†–û–°–¢–û–ï –û–ë–†–ê–¢–ù–û–ï –ü–†–ï–û–ë–†–ê–ó–û–í–ê–ù–ò–ï
        function unicodeToAscii(text) {
            if (!text) return "";
            
            let result = text;
            
            for (let styleIndex = 1; styleIndex < styles.length -1; styleIndex++) { // –ó–¥–µ—Å—å —É—á–∏—Ç—ã–≤–∞–µ–º —Ç–æ—Ç —Ñ–∞–∫—Ç, —á—Ç–æ –ø–æ—Å–ª–µ–¥–Ω–∏–π —Å—Ç–∏–ª—å - —Å–ª—É—á–∞–π–Ω—ã–π!
                const unicodeSymbols = styles[styleIndex].symbols;
                const asciiSymbols = styles[0].symbols;
                
                for (let i = 0; i < unicodeSymbols.length; i++) {
                    result = result.split(unicodeSymbols[i]).join(asciiSymbols[i]);
                }
            }
            
            return result;
        }

        // –ü—Ä–µ–æ–±—Ä–∞–∑–æ–≤–∞–Ω–∏–µ —Ç–µ–∫—Å—Ç–∞
        function convertText() {
            const editor = document.getElementById('unifiedEditor');
            const input = editor.value;
            const styleIndex = parseInt(document.getElementById('styleSelect').value);
            const result = ascii2Unicode(input, styleIndex);
            
            editor.value = result;
        }

        // –ö–æ–ø–∏—Ä–æ–≤–∞–Ω–∏–µ –≤ –±—É—Ñ–µ—Ä –æ–±–º–µ–Ω–∞ (—Å multilingual —Å–æ–æ–±—â–µ–Ω–∏—è–º–∏)
        function copyToClipboard() {
            const editor = document.getElementById('unifiedEditor');
            const result = editor.value;
            const t = translations[currentLang];
            
            if (navigator.clipboard && navigator.clipboard.writeText) {
                navigator.clipboard.writeText(result).then(() => {
                    alert(t.copySuccess);
                }).catch(() => {
                    fallbackCopyToClipboard(result);
                });
            } else {
                fallbackCopyToClipboard(result);
            }
        }

        // –†–µ–∑–µ—Ä–≤–Ω—ã–π –º–µ—Ç–æ–¥ –¥–ª—è –º–æ–±–∏–ª—å–Ω—ã—Ö
        function fallbackCopyToClipboard(text) {
            const t = translations[currentLang];
            const textArea = document.createElement('textarea');
            textArea.value = text;
            textArea.style.position = 'fixed';
            textArea.style.opacity = '0';
            document.body.appendChild(textArea);
            textArea.select();
            
            try {
                document.execCommand('copy');
                alert(t.copySuccess);
            } catch (err) {
                alert(t.copyError + err);
            }
            
            document.body.removeChild(textArea);
        }

        // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –ø—Ä–∏ –∑–∞–≥—Ä—É–∑–∫–µ
        document.addEventListener('DOMContentLoaded', function() {
            initializeStyles();
            updateInterfaceTexts(); // –¢–µ–ø–µ—Ä—å —ç—Ç–∞ —Ñ—É–Ω–∫—Ü–∏—è –æ–±–Ω–æ–≤–ª—è–µ—Ç –∏ QR-–±–ª–æ–∫ —Ç–æ–∂–µ
        });
    </script>

